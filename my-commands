  902  git init
  903  git add ./index.html ./assets/styles.css
  904  git status
  905  git commit -m "added stylesheet and index"
  906  git remote add origin https://github.com/Vasteward/Lesson-2-Drill-2.git
  907  git push -u origin master
  908  git add -A
  909  git commit -m "Remote push"
  910  git push -u origin master
  911  git pull origin master
  912  history | tail -n > my-commands
